/************************************************************************************** * CODE FILE: rCalculatorUserTypes.c++ * PURPOSE	: define members of token_name class & calculator_symbol class **************************************************************************************/#include "rCalculatorUserTypes.h"// Define Default Constructortoken_name::token_name(){	name="";	token=0;}// Define Parameterized Constructortoken_name::token_name(const char *tokenname, const char token_character){	name=tokenname;	token=token_character;}// Define overloaded output operatorostream& operator<<(ostream& output_stream, const token_name output){	return output_stream << output.name << " -> " << output.token; }// Define method for initialising all data membersvoid calculator_symbol::clear(){	token=NAME;	name_string="";	number_value=complex (0,0);}		