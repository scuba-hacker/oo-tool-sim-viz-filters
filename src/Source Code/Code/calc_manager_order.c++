#include "calc_manager_order.h"calc_manager_order::calc_manager_order(){	calculator_name="";	instruction="";}calc_manager_order::~calc_manager_order(){	// no deallocating to do	}calc_manager_order::calc_manager_order(const calc_manager_order &original){	calculator_name=original.calculator_name;	instruction=original.instruction;}calc_manager_order& calc_manager_order::operator=(calc_manager_order &source){	calculator_name=source.calculator_name;	instruction=source.instruction;	return *this;}